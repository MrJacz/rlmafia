	"scripts": {
		"sapphire": "bunx sapphire",
		"generate": "bunx sapphire generate",
		"build": "bunx tsc",
		"typecheck": "bunx tsc --noEmit",
		"watch": "bunx tsc -w",
		"start": "bun src/index.ts",
		"dev": "bun --watch src/index.ts",
		"watch:start": "bunx tsc-watch --onSuccess \"bun run start\"",
		"check": "bunx biome check --write .",
