		"colorette": "^2.0.20",
		"discord.js": "^14.25.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@sapphire/cli": "^1.9.3",
		"@sapphire/ts-config": "^5.0.3",
		"@types/bun": "^1.3.3",
		"@types/node": "^24.10.1",
		"@types/ws": "^8.18.1",
		"prisma": "^7.1.0",
